module unhide_vehicle_upgrade_level

def Init(self):
    pass

def Trigger(self):
    local1 = scriptgo.GetProperties(self)
    local2 = game.GetGameObjectByAlias("garage.state")
    if not local2:
        debug.LogInfo("unhide vehicle upgrade level script: no garage state found.", [])
        return
    garagestate.UnhideAttributeLevel(local2, game.GameHashString(local1.attr_id), local1.level)
